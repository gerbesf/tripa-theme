{
    "name": "gerbesf/tripa-theme",
    "description": "Layout do apps",
    "keywords": [
        "gerbesf",
        "tripa-theme"
    ],
    "homepage": "https://github.com/gerbesf/tripa-theme",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Gerbes Matos",
            "email": "gerbes.matos@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Gerbesf\\TripaTheme\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gerbesf\\TripaTheme\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gerbesf\\TripaTheme\\TripaThemeServiceProvider"
            ],
            "aliases": {
                "TripaTheme": "Gerbesf\\TripaTheme\\TripaThemeFacade"
            }
        }
    }
}
